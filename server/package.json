{
  "name": "create-kuji-server",
  "version": "1.0.0",
  "description": "Backend API server for Create Kuji App with authentication and user management",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "start": "node dist/index.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:migrate:reset": "prisma migrate reset --force",
    "prisma:seed": "ts-node src/utils/seed.ts",
    "prisma:studio": "prisma studio",
    "db:setup": "prisma migrate dev --name init && prisma generate && npm run prisma:seed",
    "db:setup-windows": "powershell -ExecutionPolicy Bypass -File ./setup-db-windows.ps1",
    "db:setup-unix": "bash ./setup-db-unix.sh",
    "db:reset": "prisma migrate reset --force && npm run prisma:seed",
    "db:push": "prisma db push",
    "db:pull": "prisma db pull",
    "db:dev": "prisma migrate dev && npm run prisma:seed",
    "fix:demo": "ts-node src/utils/fix-demo-username.ts",
    "demo:permanent": "ts-node src/utils/make-demo-username-permanent.ts",
    "support:change-username": "ts-node src/utils/change-username.ts",
    "support:change-username-email": "ts-node src/utils/change-username-by-email.ts",
    "support:upgrade-to-pro": "ts-node src/utils/upgrade-to-pro.ts",
    "fix:demo-plan": "ts-node src/utils/check-and-fix-demo-plan.ts",
    "fix:publish-demo-stock": "ts-node src/utils/publish-demo-stock.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": ["nodejs", "express", "typescript", "prisma", "authentication", "kuji", "gacha"],
  "author": "Kyros Koh <kyroskoh@users.noreply.github.com>",
  "license": "MIT",
  "type": "commonjs",
  "dependencies": {
    "@prisma/client": "^6.16.3",
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^5.0.3",
    "@types/helmet": "^0.0.48",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.6.2",
    "@types/nodemailer": "^7.0.2",
    "@types/passport": "^1.0.17",
    "@types/passport-discord": "^0.1.14",
    "@types/passport-facebook": "^3.0.3",
    "@types/passport-github2": "^1.2.9",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-linkedin-oauth2": "^1.5.6",
    "@types/passport-local": "^1.0.38",
    "@types/passport-twitter": "^1.0.40",
    "@types/pg": "^8.15.5",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^7.0.6",
    "passport": "^0.7.0",
    "passport-discord": "^0.1.4",
    "passport-facebook": "^3.0.0",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-linkedin-oauth2": "^2.0.0",
    "passport-local": "^1.0.0",
    "passport-twitter": "^0.1.5",
    "pg": "^8.16.3",
    "prisma": "^6.16.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}
